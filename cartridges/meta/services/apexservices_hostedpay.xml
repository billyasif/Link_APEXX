<?xml version="1.0" encoding="UTF-8"?>
<services xmlns="http://www.demandware.com/xml/impex/services/2014-09-26">
    <service-credential service-credential-id="apexx.https.hostedpay">
        <url>https://sandmgw.apexxfintech.com/mgw/payment/hosted</url>
    </service-credential>
   
   <service-profile service-profile-id="apexx.https.hostedpay">
        <timeout-millis>10000</timeout-millis>
        <rate-limit-enabled>false</rate-limit-enabled>
        <rate-limit-calls>0</rate-limit-calls>
        <rate-limit-millis>0</rate-limit-millis>
        <cb-enabled>false</cb-enabled>
        <cb-calls>0</cb-calls>
        <cb-millis>0</cb-millis>
    </service-profile>

    <service service-id="apexx.https.hostedpay">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>APEXX_API_LOG</log-prefix>
        <comm-log-enabled>false</comm-log-enabled>
        <force-prd-enabled>true</force-prd-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>apexx.https.hostedpay</profile-id>
        <credential-id>apexx.https.hostedpay</credential-id>
    </service>

    </services>
